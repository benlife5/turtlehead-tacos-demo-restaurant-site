{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "api/[id]",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/{{id}}",
        "sourceFiles": [
          {
            "root": "dist",
            "pattern": "assets/functions/http/api/\\[id\\].*.*"
          }
        ]
      },
      {
        "pluginName": "api/[userId]/profile",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/{{userId}}/profile",
        "sourceFiles": [
          {
            "root": "dist",
            "pattern": "assets/functions/http/api/\\[userId\\]/profile.*.*"
          }
        ]
      },
      {
        "pluginName": "api/helloWorld1",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/helloWorld1",
        "sourceFiles": [
          {
            "root": "dist",
            "pattern": "assets/functions/http/api/helloWorld1.*.*"
          }
        ]
      },
      {
        "pluginName": "helloWorld2",
        "event": "API",
        "functionName": "default",
        "apiPath": "helloWorld2",
        "sourceFiles": [
          {
            "root": "dist",
            "pattern": "assets/functions/http/helloWorld2.*.*"
          }
        ]
      },
      {
        "pluginName": "gen",
        "event": "ON_PAGE_GENERATE",
        "functionName": "default",
        "sourceFiles": [
          {
            "root": "dist",
            "pattern": "assets/functions/onPageGenerate/gen.*.*"
          }
        ]
      },
      {
        "pluginName": "update",
        "event": "ON_URL_CHANGE",
        "functionName": "default",
        "sourceFiles": [
          {
            "root": "dist",
            "pattern": "assets/functions/onUrlChange/update.*.*"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".npmrc"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}
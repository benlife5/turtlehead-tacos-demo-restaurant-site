{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "id-38826",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/{{id}}",
        "sourceFiles": [
          {
            "root": "dist/functions/id-38826",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "profile.1-18088",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/{{userId}}/profile.1",
        "sourceFiles": [
          {
            "root": "dist/functions/profile.1-18088",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "helloWorld-29361",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/helloWorld",
        "sourceFiles": [
          {
            "root": "dist/functions/helloWorld-29361",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "id-84874",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/id",
        "sourceFiles": [
          {
            "root": "dist/functions/id-84874",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "helloWorld-22074",
        "event": "API",
        "functionName": "default",
        "apiPath": "helloWorld",
        "sourceFiles": [
          {
            "root": "dist/functions/helloWorld-22074",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "helloWorld-79138",
        "event": "ON_URL_CHANGE",
        "functionName": "default",
        "sourceFiles": [
          {
            "root": "dist/functions/helloWorld-79138",
            "pattern": "*{.js,.ts}"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".npmrc"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}
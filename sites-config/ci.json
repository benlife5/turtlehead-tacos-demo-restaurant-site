{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "id-91043",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/{{id}}",
        "sourceFiles": [
          {
            "root": "dist/functions/id-91043",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "profile.1-62187",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/{{userId}}/profile.1",
        "sourceFiles": [
          {
            "root": "dist/functions/profile.1-62187",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "helloWorld-38172",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/helloWorld",
        "sourceFiles": [
          {
            "root": "dist/functions/helloWorld-38172",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "id-69911",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/id",
        "sourceFiles": [
          {
            "root": "dist/functions/id-69911",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "helloWorld-88487",
        "event": "API",
        "functionName": "default",
        "apiPath": "helloWorld",
        "sourceFiles": [
          {
            "root": "dist/functions/helloWorld-88487",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "helloWorld-19025",
        "event": "ON_URL_CHANGE",
        "functionName": "default",
        "sourceFiles": [
          {
            "root": "dist/functions/helloWorld-19025",
            "pattern": "*{.js,.ts}"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".npmrc"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}